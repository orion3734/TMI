<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.TimelineMapper">
	<select id="timeList" resultType="dto.TimelineDTO"  parameterType="string">
		select * from timeline where pro_id =  #{pro_id}  order by TL_TIME desc 
	</select>
	

</mapper>
